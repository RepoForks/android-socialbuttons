<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FacebookSharesButton">
        <attr name="sharesUrl" format="string" />
        <attr name="type">
            <enum name="normal" value="0" />
            <enum name="medium" value="1" />
        </attr>
        <attr name="annotation">
            <enum name="none" value="0" />
            <enum name="bubble" value="1" />
            <enum name="bubble_onload" value="2" />
        </attr>
    </declare-styleable>
</resources>